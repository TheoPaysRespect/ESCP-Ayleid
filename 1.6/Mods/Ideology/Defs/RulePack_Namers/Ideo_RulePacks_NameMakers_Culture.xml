<?xml version="1.0" encoding="utf-8" ?>
<Defs>
	<!-- ==========[ESCP_Ayleid_NamerCultureLeaderTitle]========== -->
	<RulePackDef>
		<defName>ESCP_Ayleid_NamerCultureLeaderTitle</defName>
		<include>
			<li>LeaderTitleMaker_GlobalStructure</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>memeLeaderNoun(priority=1)->[cultureLeaderNoun]</li>
				<!-- ignore all meme nouns, tribals always use a culture noun -->
				<li>cultureLeaderTitle->[cultureLeaderAdjective] [cultureLeaderNoun]</li>
				<li>cultureLeaderAdjective->great</li>
				<li>cultureLeaderAdjective->high</li>
				<li>cultureLeaderAdjective->first</li>
				<li>cultureLeaderAdjective->grand</li>
				<li>cultureLeaderAdjective->knight</li>
				<li>cultureLeaderNoun->king</li>
			</rulesStrings>
		</rulePack>
	</RulePackDef>
	<!-- ==========[ESCP_Ayleid_NamerCultureDeityName]========== -->
	<RulePackDef>
		<defName>ESCP_Ayleid_NamerCultureDeityName</defName>
		<include>
			<li>NamerDeityGlobal</li>
		</include>
		<rulePack>
			<rulesStrings>
				<li>name->[AyleidGods]</li>
			</rulesStrings>
			<rulesFiles>
				<li>AyleidGods->ESCP_AyleidNames/AyleidGods</li>
			</rulesFiles>
		</rulePack>
	</RulePackDef>
</Defs>
